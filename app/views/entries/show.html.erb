<p id="notice"><%= notice %></p>
<h2>Show Entry</h2>
<p>
  <strong>Date:</strong>
  <%= @entry.date %><br>

  <strong>Note:</strong>
  <%= @entry.note %>
</p>

<%= link_to 'Edit', edit_entry_path(@entry), class: 'btn btn-info gtn-md' %>
<%= link_to 'Back to List of Entries', entries_path, class: 'btn btn-info btn-md' %>
<td><%= link_to 'Delete Entry', @entry, method: :delete, data: { confirm: 'Are you sure?' }, class:'btn btn-danger btn-md' %></td>
